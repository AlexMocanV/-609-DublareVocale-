#include <cstring>
#include <iostream>
int main()
{std::string c;
    getline(std::cin,c);
    for(int i=0;i<c.length();i++)
    {
        std::cout<<c[i];
        if(c[i]=='a'||c[i]=='o'||c[i]=='u'||c[i]=='e'||c[i]=='i'||c[i]=='A'||c[i]=='O'||c[i]=='U'||c[i]=='E'||c[i]=='I')
        {
            std::cout<<c[i];
        }
    }
    return 0;
}
